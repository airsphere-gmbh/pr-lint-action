  },
  "homepage": "https://github.com/MorrisonCole/pr-lint-action#readme",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/github": "4.0.0"
  },
  "devDependencies": {
    "@types/node": "14.14.6",
